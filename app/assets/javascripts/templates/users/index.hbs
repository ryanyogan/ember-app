<h1>Users</h1>
<table class='table table-striped'>
  <tr>
    <th>ID</th>
    <th>Name</th>
  </tr>
  {{#each user in users}}
    <tr>
      <td>{{user.id}}</td>
      <td>{{#linkTo 'users.show' user}}{{user.fullName}}{{/linkTo}}</td>
    </tr>
    {{/each}}
</table>

{{#linkTo 'users.new' class='btn btn-primary'}}Create new user{{/linkTo}}

